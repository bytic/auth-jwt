    }
  },
  "require": {
    "php": ">=7.1",
    "bytic/container": "^0.9|^1.0",
    "bytic/auth": "^0.9|^1.0",
    "symfony/security": "^4.4",
    "lcobucci/jwt": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.1"
  }
}
