{
  "name": "bytic/jwt-auth",
  "description": "JWT Auth Classes used by ByTIC Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\JWTAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\JWTAuth\\Tests\\": "tests/src",
      "ByTIC\\JWTAuth\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=7.0",
    "bytic/auth": "^0.9|^1.0",
    "symfony/security": "^4.4",
    "lcobucci/jwt": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  }
}
